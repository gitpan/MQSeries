void
MQPUT(Hconn,Hobj,MsgDesc,PutMsgOpts,Msg,CompCode,Reason)
	MQHCONN	Hconn
	MQHOBJ	Hobj
	MQMD	MsgDesc
	MQPMO	PutMsgOpts
	SV *	Msg
	MQLONG	CompCode
	MQLONG	Reason

	CODE:
	{
	PMQCHAR Buffer = SvPV(Msg,na);
	MQPUT(Hconn,Hobj,&MsgDesc,&PutMsgOpts,SvCUR(Msg),Buffer,&CompCode,&Reason);
	}
	OUTPUT:
	MsgDesc
	PutMsgOpts
	CompCode
	Reason
