void
MQDLTMH(Hconn,Hmsg,DltMsgHOpts,CompCode,Reason)
        MQHCONN Hconn
        MQHMSG  Hmsg
        MQDMHO  DltMsgHOpts
        MQLONG  CompCode = NO_INIT
        MQLONG  Reason = NO_INIT

    CODE:
        MQDLTMH(Hconn,&Hmsg,&DltMsgHOpts,&CompCode,&Reason);
    OUTPUT:
        Hmsg
        CompCode
        Reason

