void
MQPUT1(Hconn,ObjDesc,MsgDesc,PutMsgOpts,Msg,CompCode,Reason)
	MQHCONN	Hconn
	MQOD	ObjDesc
	MQMD	MsgDesc
	MQPMO	PutMsgOpts
	SV *	Msg
	MQLONG	CompCode
	MQLONG	Reason

	CODE:
	{
	PMQCHAR Buffer = SvPV(Msg,na);
	MQPUT1(Hconn,&ObjDesc,&MsgDesc,&PutMsgOpts,SvCUR(Msg),Buffer,&CompCode,&Reason);
	}
	OUTPUT:
	ObjDesc
	MsgDesc
	PutMsgOpts
	CompCode
	Reason
