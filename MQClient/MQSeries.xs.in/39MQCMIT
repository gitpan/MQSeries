void
MQCMIT(Hconn,CompCode,Reason)
	MQHCONN	Hconn
	MQLONG	CompCode
	MQLONG	Reason

	CODE:
	MQCMIT(Hconn,&CompCode,&Reason);
	OUTPUT:
	Hconn
	CompCode
	Reason

