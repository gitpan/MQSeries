void
MQCMIT(Hconn,CompCode,Reason)
	MQHCONN	Hconn
	MQLONG	CompCode = NO_INIT
	MQLONG	Reason = NO_INIT

    CODE:
	MQCMIT(Hconn,&CompCode,&Reason);
    OUTPUT:
	Hconn
	CompCode
	Reason

