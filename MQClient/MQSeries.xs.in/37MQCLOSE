void
MQCLOSE(Hconn,Hobj,Options,CompCode,Reason)
	MQHCONN	Hconn
	MQHOBJ	Hobj
	MQLONG	Options
	MQLONG	CompCode = NO_INIT
	MQLONG	Reason = NO_INIT

    CODE:
	MQCLOSE(Hconn,&Hobj,Options,&CompCode,&Reason);
    OUTPUT:
	Hobj
	CompCode
	Reason

