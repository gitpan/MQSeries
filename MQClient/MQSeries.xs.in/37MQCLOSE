void
MQCLOSE(Hconn,Hobj,Options,CompCode,Reason)
	MQHCONN	Hconn
	MQHOBJ	Hobj
	MQLONG	Options
	MQLONG	CompCode
	MQLONG	Reason

	CODE:
	MQCLOSE(Hconn,&Hobj,Options,&CompCode,&Reason);
	OUTPUT:
	Hobj
	CompCode
	Reason

