void
MQBACK(Hconn,CompCode,Reason)
	MQHCONN	Hconn
	MQLONG	CompCode
	MQLONG	Reason

	CODE:
	MQBACK(Hconn,&CompCode,&Reason);
	OUTPUT:
	Hconn
	CompCode
	Reason

