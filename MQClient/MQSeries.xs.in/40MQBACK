MQBACK(Hconn,CompCode,Reason)
	MQHCONN	Hconn
	MQLONG	CompCode = NO_INIT
	MQLONG	Reason = NO_INIT

    CODE:
	MQBACK(Hconn,&CompCode,&Reason);
