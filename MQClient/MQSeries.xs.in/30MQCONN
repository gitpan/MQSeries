MQHCONN
MQCONN(Name,CompCode,Reason)
	MQCHAR48	Name
	MQLONG		CompCode
	MQLONG		Reason

	CODE:
	MQCONN(Name,&RETVAL,&CompCode,&Reason);
	OUTPUT:
	RETVAL
	CompCode
	Reason

