#ifdef MQCNO_NONE

MQHCONN
MQCONNX(Name,ConnectOpts,CompCode,Reason)
	MQCHAR48	Name
	MQCNO		ConnectOpts
	MQLONG		CompCode
	MQLONG		Reason

	CODE:
	MQCONNX(Name,&ConnectOpts,&RETVAL,&CompCode,&Reason);
	OUTPUT:
	ConnectOpts
	RETVAL
	CompCode
	Reason

#endif

