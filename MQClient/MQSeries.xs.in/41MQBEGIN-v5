#ifdef MQBO_NONE

void
MQBEGIN(Hconn,BeginOpts,CompCode,Reason)
	MQHCONN		Hconn
	MQBO		BeginOpts
	MQLONG		CompCode
	MQLONG		Reason

	CODE:
	MQBEGIN(Hconn,
		&BeginOpts,
		&CompCode,
		&Reason);
	OUTPUT:
	BeginOpts
	CompCode
	Reason

#endif
