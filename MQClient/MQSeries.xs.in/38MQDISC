void
MQDISC(Hconn,CompCode,Reason)
	MQHCONN	Hconn
	MQLONG	CompCode
	MQLONG	Reason

	CODE:
	MQDISC(&Hconn,&CompCode,&Reason);
	OUTPUT:
	Hconn
	CompCode
	Reason

