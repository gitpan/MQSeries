void
MQDISC(Hconn,CompCode,Reason)
	MQHCONN	Hconn
	MQLONG	CompCode = NO_INIT
	MQLONG	Reason = NO_INIT

    CODE:
	MQDISC(&Hconn,&CompCode,&Reason);
    OUTPUT:
	Hconn
	CompCode
	Reason

